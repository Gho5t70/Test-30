class Solution {
public:
    bool searchMatrix(vector<vector<int>>& matrix, int target) {
     int i;int j,m=matrix.size();int n=matrix[0].size();
        if(m==0)
            return false;
	for(i=0;i<m;i++){
        for(j=0;j<n;j++){
		if(matrix[i][j]==target)
		return true;
        }
    }
     return false;   
    }
};
